{
    "entryPoints": [
        "./build/client/typescript/index.ts",
    ],
    "out": "./build/client/typescript/docs",
    "plugin": [
        "typedoc-plugin-markdown",
        "typedoc-vitepress-theme"
    ],
    "docsRoot": "./build/client/typescript/docs",
    "readme": "none",
    "indexFormat": "table",
    "useCodeBlocks": true,
    "disableSources": true,
    "suppressCommentWarningsInDeclarationFiles": true
}